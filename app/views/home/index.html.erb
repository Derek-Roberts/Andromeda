<div id="container">

    <% for user in @users %>
      <div class="user">
        <p>
          <strong><%=h user.username %></strong>
          <%= link_to "Add Friend", friend_path(:friend_id => user), :method => :post %>
          <div class="clear"></div>
        </p>
      </div>
    <% end %>

</div>
